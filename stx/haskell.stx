displayname: haskell
extensions: .*\.hs$
comment|no_sep: --.*$
keyword: (!|-<|-<|->|::|<-|=>|#|@|[\||\|]|_|as|case|of|class|data(\s+family)?|default|deriving|do|forall|foreign|hiding|if|then|else|import|infix(l|r)?|instance|let|in|mdo|module|newtype|proc|qualified|rec|type(\s+family)?|where)
type: [A-Z][a-zA-Z]*
comment|no_sep: \{-.*-\}
comment|no_sep: \{-.*
                 .*-\}
pragma: `.*`
string|no_sep: '([^\\\']|\\.)'
string|no_sep: "([^\\\"]|\\.)*"
number: [+-]?([0-9]+([.][0-9]*)?|[.][0-9]+)
